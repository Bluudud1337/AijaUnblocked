{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/(manifest.json)$",
      "dest": "/public/$1"
    },
    {
      "src": "^/(sw.js)$",
      "dest": "/public/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/public/$1"
    }
  ]
}
